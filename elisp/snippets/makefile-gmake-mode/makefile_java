# -*- mode: snippet -*-
# name: java
# key: java
# expand-env: ((yas-indent-line 'fixed))
# --
JFLAGS = -g -Xdiags:verbose
JC = javac
.SUFFIXES: .java .class
.java.class:
	$(JC) $(JFLAGS) $(CLASSES)

CLASSES = \
# file.java \

default: classes

classes: $(CLASSES:.java=.class)

clean:
	$(RM) *.class
